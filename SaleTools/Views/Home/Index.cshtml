                }
                for (var k in topBox) {
                    diff = topBox[k] - scrollTop - k * 36 + 30;
                    if (diff < 240) {
                        showKey = k;
                    }
                }
                $("#J_LeftNav li").each(function (index) {
                    if (index == showKey) {
                        $(this).addClass("selected");
                    } else {
                        $(this).removeClass("selected");
