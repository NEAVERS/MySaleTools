                if (max < min) {
                    build("red", "商品库存不足", "Increment");
                    return false;
                }
                var quantity = parseInt(_input.val());
                ++quantity;
                quantity = quantity > max ? max : quantity;
                quantity = quantity < min ? min : quantity;
                _input.val(quantity);
            });
            $(".J_QuantityInput").on("blur", function () {
