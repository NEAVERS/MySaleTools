        $(function () {
            var topBox = [];
            $(".floor").each(function () {
                topBox.push($(this).offset().top);
            });

            $(window).on("scroll", function () {
                var scrollTop = $(document).scrollTop();
                var showKey = -1;
                var diff;
                if (scrollTop > 340) {
                    $("#J_LeftNav").css({ display: "block" });
                    $(".marq").css({ position: 'fixed', top: '40px' }) //跑马灯 可以在每次更新维护的时候用
